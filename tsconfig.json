{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "outDir": "dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strict": true,
        "skipLibCheck": true /* Skip type checking all .d.ts files. */
    }
}
